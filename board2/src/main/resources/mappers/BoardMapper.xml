<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.board2.mappers.BoardMapper">
	<select id="selectList" resultType="com.spring.board2.domain.BoardVO">
		SELECT
			ID,
			WRITER,
			TITLE,
			CONTENT,
			PASSWORD,
			REG_DATE
		FROM BOARD
	</select>

</mapper>